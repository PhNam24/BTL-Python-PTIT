<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.flowdev.tmadposter">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:name="ParseApplication" android:theme="@style/AppTheme">
        <meta-data android:name="com.google.android.gms.version" android:value="@integer/google_play_services_version"/>
        <activity android:label="@string/app_name" android:launchMode="singleTask" android:name="com.flowdev.tmadposter.SplashActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:launchMode="singleTask" android:name="com.flowdev.tmadposter.MainActivity" android:screenOrientation="portrait" android:uiOptions="splitActionBarWhenNarrow"/>
        <activity android:label="@string/action_about" android:launchMode="singleTask" android:name="com.flowdev.tmadposter.AboutActivity" android:screenOrientation="portrait" android:uiOptions="splitActionBarWhenNarrow"/>
        <activity android:label="Picking Mate" android:launchMode="singleTop" android:name="com.flowdev.tmadposter.AboutPickingMateActivity" android:screenOrientation="portrait" android:uiOptions="splitActionBarWhenNarrow"/>
        <activity android:label="@string/action_contact" android:launchMode="singleTask" android:name="com.flowdev.tmadposter.ContactActivity" android:screenOrientation="portrait" android:uiOptions="splitActionBarWhenNarrow" android:windowSoftInputMode="adjustPan"/>
        <activity android:label="@string/action_postjob" android:launchMode="singleTask" android:name="com.flowdev.tmadposter.PostJobActivity" android:screenOrientation="portrait" android:uiOptions="splitActionBarWhenNarrow" android:windowSoftInputMode="adjustPan"/>
        <activity android:label="@string/label_jobsposted" android:launchMode="singleTask" android:name="com.flowdev.tmadposter.JobsPostedActivity" android:screenOrientation="portrait" android:uiOptions="splitActionBarWhenNarrow" android:windowSoftInputMode="adjustPan"/>
        <activity android:label="@string/label_jobdetails" android:launchMode="singleTop" android:name="com.flowdev.tmadposter.JobDetailsActivity" android:screenOrientation="portrait" android:uiOptions="splitActionBarWhenNarrow" android:windowSoftInputMode="adjustPan"/>
        <activity android:label="@string/label_editprofile" android:launchMode="singleTop" android:name="com.flowdev.tmadposter.EditProfileActivity" android:screenOrientation="portrait" android:uiOptions="splitActionBarWhenNarrow" android:windowSoftInputMode="adjustPan"/>
        <activity android:label="@string/label_login" android:launchMode="singleTask" android:name="com.flowdev.tmadposter.LoginActivity" android:screenOrientation="portrait" android:uiOptions="splitActionBarWhenNarrow" android:windowSoftInputMode="adjustPan"/>
        <activity android:label="@string/label_signup" android:launchMode="singleTop" android:name="com.flowdev.tmadposter.SignupActivity" android:screenOrientation="portrait" android:uiOptions="splitActionBarWhenNarrow" android:windowSoftInputMode="adjustPan"/>
        <activity android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|screenSize|smallestScreenSize|uiMode" android:name="com.google.android.gms.ads.AdActivity"/>
    </application>
</manifest>