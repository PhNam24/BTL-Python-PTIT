<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" android:installLocation="auto" package="com.veronikadev.wolf" platformBuildVersionCode="23" platformBuildVersionName="6.0-2704002">
