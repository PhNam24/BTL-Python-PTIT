        <activity android:configChanges="keyboardHidden|orientation" android:name="com.antiy.avl.CooperationActivity" android:screenOrientation="portrait"/>
        <activity android:configChanges="keyboardHidden|orientation" android:label="" android:name="com.antiy.avl.SettingActivity" android:screenOrientation="portrait"/>
