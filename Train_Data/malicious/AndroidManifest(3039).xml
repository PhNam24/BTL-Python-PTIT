<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" android:installLocation="preferExternal" package="com.azaleastudio">
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:label="@string/app_name" android:name=".WordPadApp" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/books" android:name=".BookList" android:screenOrientation="portrait"/>
        <activity android:label="@string/words" android:name=".WordsList" android:screenOrientation="portrait"/>
        <activity android:label="@string/groups" android:name=".GroupList" android:screenOrientation="portrait"/>
        <activity android:label="@string/assignments" android:name=".Assignments" android:screenOrientation="portrait"/>
        <activity android:label="@string/word_pad" android:name=".WordPad" android:screenOrientation="portrait"/>
        <activity android:label="@string/user_guide" android:name=".UserGuide" android:screenOrientation="portrait"/>
        <activity android:label="@string/dictionary" android:name=".Dictionary" android:screenOrientation="portrait"/>
        <activity android:label="@string/setting" android:name=".Setting" android:screenOrientation="portrait"/>
        <activity android:label="@string/browse" android:name=".FileBrowser" android:screenOrientation="portrait"/>
        <activity android:label="@string/create_book" android:name=".CreateBook" android:screenOrientation="portrait"/>
        <activity android:configChanges="keyboard|keyboardHidden|orientation" android:name="net.youmi.android.AdActivity"/>
    </application>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
</manifest>