        <service android:name="org.geometerplus.android.fbreader.network.ItemsLoadingService" android:process=":networkLibrary"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="org.geometerplus.android.fbreader.network.NetworkBookInfoActivity" android:process=":networkLibrary">
