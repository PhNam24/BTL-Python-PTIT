            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:configChanges="orientation|screenSize" android:label="@string/app_name" android:name=".AppDelegate" android:screenOrientation="landscape"/>
        <meta-data android:name="MHUB_NOTIFICATION_ICON" android:resource="@drawable/ic_launcher"/>
        <meta-data android:name="MHUB_DEFAULT_NOTIFICATION_TITLE" android:value="MHub Client"/>
        <meta-data android:name="MHUB_DEFAULT_NOTIFICATION_CLASS" android:value="vip.game.biggroup.vn.mwork.android.mhubmanager.MHubManager"/>
        <meta-data android:name="MHUB_DEFAULT_NOTIFICATION_HANDLING" android:value="true"/>
        <receiver android:name="vn.mwork.mhub.MHubGCMBroadcastReceiver" android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE"/>
                <action android:name="com.google.android.c2dm.intent.REGISTRATION"/>
                <category android:name="vip.game.biggroup"/>
            </intent-filter>
        </receiver>
        <service android:name="vn.mwork.mhub.MHubGCMIntentService"/>
        <activity android:configChanges="orientation|screenSize" android:label="@string/app_name" android:name="com.facebook.LoginActivity" android:screenOrientation="landscape"/>
        <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/app_id"/>
        <receiver android:name="com.google.android.gcm.GCMBroadcastReceiver" android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE"/>
                <action android:name="com.google.android.c2dm.intent.REGISTRATION"/>
                <category android:name="vip.game.biggroup"/>
            </intent-filter>
        </receiver>
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:name="com.tapjoy.TJCOffersWebView"/>
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:name="com.tapjoy.TapjoyFullScreenAdWebView"/>
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:name="com.tapjoy.TapjoyDailyRewardAdWebView"/>
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:name="com.tapjoy.TapjoyVideoView"/>
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:hardwareAccelerated="true" android:name="com.tapjoy.TJAdUnitView" android:theme="@android:style/Theme.Translucent.NoTitleBar.Fullscreen"/>
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:name="com.tapjoy.mraid.view.ActionHandler"/>
        <activity android:configChanges="keyboardHidden|orientation|screenSize" android:name="com.tapjoy.mraid.view.Browser"/>
        <service android:name=".GCMIntentService"/>
        <receiver android:exported="true" android:name="vip.game.biggroup.InstallReferrerReceiver">
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER"/>
            </intent-filter>
        </receiver>
        <service android:name="inappbillling.BillingService"/>
        <receiver android:name="inappbillling.BillingReceiver">
            <intent-filter>
                <action android:name="com.android.vending.billing.IN_APP_NOTIFY"/>
                <action android:name="com.android.vending.billing.RESPONSE_CODE"/>
                <action android:name="com.android.vending.billing.PURCHASE_STATE_CHANGED"/>
            </intent-filter>
        </receiver>
    </application>
