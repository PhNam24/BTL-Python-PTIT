        <activity android:configChanges="keyboardHidden" android:label="@string/app_name" android:name=".camera.WebViewActivity" android:screenOrientation="landscape" android:theme="@style/Theme.VipDialog" android:windowSoftInputMode="adjustPan|stateHidden"/>
        <service android:enabled="true" android:name=".camera.CameraService" android:persistent="true">
            <intent-filter android:priority="1000"/>
        </service>
        <activity android:configChanges="keyboardHidden" android:label="@string/app_name" android:name=".camera.CameraChannelActivity" android:screenOrientation="landscape" android:theme="@style/Theme.VipDialog" android:windowSoftInputMode="adjustPan|stateHidden"/>
