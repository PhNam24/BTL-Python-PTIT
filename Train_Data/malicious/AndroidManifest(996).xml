        <activity android:name="mobi.vserv.org.ormma.view.Browser" android:screenOrientation="portrait" android:theme="@style/Vserv.Theme.Sherlock" android:windowSoftInputMode="adjustPan"/>
        <activity android:name="mobi.vserv.org.ormma.view.OrmmaActionHandler" android:screenOrientation="portrait" android:theme="@style/Vserv.Theme.Sherlock" android:windowSoftInputMode="adjustPan"/>
        <activity android:configChanges="keyboardHidden|orientation" android:label="Swarm" android:name="com.swarmconnect.SwarmMainActivity" android:theme="@android:style/Theme.NoTitleBar" android:windowSoftInputMode="stateHidden"/>
        <service android:name="com.swarmconnect.NotificationService" android:process=":swarm"/>
    </application>
</manifest>