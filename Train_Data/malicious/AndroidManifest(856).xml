        <activity android:configChanges="keyboard|keyboardHidden|navigation|orientation|screenLayout|screenSize|touchscreen|uiMode" android:launchMode="singleTop" android:name="mobi.vserv.android.ads.VservAdManager" android:process=":vserv" android:windowSoftInputMode="adjustPan"/>
