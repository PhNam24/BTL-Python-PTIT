                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="britneyspear.wallpapers.BritneyActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="britneyspear.wallpapers.SplashScreen.BritneyActivity"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name="britneyspear.wallpapers.WallPapers" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="britneyspear.wallpapers.SplashScreen.BritneyActivity.WallPapers"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:name="britneyspear.wallpapers.ExpandableListActivity" android:screenOrientation="portrait">
