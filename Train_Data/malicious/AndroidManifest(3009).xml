<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.zhuqueok.qmpyxcb">
    <uses-feature android:glEsVersion="0x00020000"/>
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <meta-data android:name="ZHUQUEOK_CHANNEL_KEY" android:value="@string/zhuqueok_channel_key"/>
        <meta-data android:name="ZHUQUEOK_CHANNEL_ID" android:value="@string/zhuqueok_channel_id"/>
        <meta-data android:name="ZHUQUEOK_APP_ID" android:value="@string/zhuqueok_app_id"/>
        <meta-data android:name="ZHUQUEOK_OTHER1" android:value="@string/zhuqueok_other1"/>
        <meta-data android:name="ZHUQUEOK_OTHER2" android:value="@string/zhuqueok_other2"/>
        <meta-data android:name="ZHUQUEOK_OTHER3" android:value="@string/zhuqueok_other3"/>
        <meta-data android:name="ZHUQUEOK_OTHER4" android:value="@string/zhuqueok_other4"/>
        <meta-data android:name="ZHUQUEOK_OTHER5" android:value="@string/zhuqueok_other5"/>
