<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.enlightenedapps.undertheseaslot">
    <uses-permission android:name="com.android.vending.BILLING"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <application android:allowBackup="true" android:icon="@drawable/icon" android:label="@string/app_name" android:largeHeap="true">
        <activity android:name="com.enlightenedapps.undertheseaslot.SplashActivity" android:screenOrientation="landscape" android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.enlightenedapps.undertheseaslot.SelectLoginActivity" android:screenOrientation="landscape" android:theme="@android:style/Theme.NoTitleBar.Fullscreen"/>
        <activity android:name="com.enlightenedapps.undertheseaslot.LoginMailActivity" android:screenOrientation="landscape" android:theme="@android:style/Theme.NoTitleBar.Fullscreen"/>
        <activity android:name="com.enlightenedapps.undertheseaslot.ChangePasswordActivity" android:screenOrientation="landscape" android:theme="@android:style/Theme.NoTitleBar.Fullscreen"/>
        <activity android:name="com.enlightenedapps.undertheseaslot.MainActivity" android:screenOrientation="landscape" android:theme="@android:style/Theme.NoTitleBar.Fullscreen"/>
        <activity android:name="com.enlightenedapps.undertheseaslot.SelectLevelActivity" android:screenOrientation="landscape" android:theme="@android:style/Theme.NoTitleBar.Fullscreen"/>
        <activity android:name="com.enlightenedapps.undertheseaslot.PlayGameActivity" android:screenOrientation="landscape" android:theme="@android:style/Theme.NoTitleBar.Fullscreen"/>
        <activity android:name="com.enlightenedapps.undertheseaslot.ShopActivity" android:screenOrientation="landscape" android:theme="@android:style/Theme.NoTitleBar.Fullscreen"/>
        <activity android:name="com.enlightenedapps.undertheseaslot.dialog.PayoutDialog" android:screenOrientation="landscape" android:theme="@style/Theme.Transparent"/>
        <activity android:name="com.enlightenedapps.undertheseaslot.dialog.HelpDialog" android:screenOrientation="landscape" android:theme="@style/Theme.Transparent"/>
        <activity android:name="com.enlightenedapps.undertheseaslot.dialog.AboutDialog" android:screenOrientation="landscape" android:theme="@style/Theme.Transparent"/>
        <activity android:name="com.enlightenedapps.undertheseaslot.dialog.BonusDialog" android:screenOrientation="landscape" android:theme="@style/Theme.Transparent"/>
        <activity android:label="@string/app_name" android:name="com.enlightenedapps.undertheseaslot.dialog.GambleDialog" android:screenOrientation="landscape" android:theme="@style/Theme.Transparent"/>
        <activity android:name="com.enlightenedapps.undertheseaslot.LoginUsingActivityActivity"/>
        <activity android:name="com.enlightenedapps.undertheseaslot.LoginUsingCustomFragmentActivity"/>
        <activity android:label="@string/app_name" android:name="com.facebook.LoginActivity" android:theme="@android:style/Theme.Translucent.NoTitleBar"/>
        <activity android:name="com.enlightenedapps.undertheseaslot.LoginUsingLoginFragmentActivity"/>
        <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/applicationId"/>
        <service android:name="com.enlightenedapps.undertheseaslot.inapp.BillingService"/>
        <receiver android:name="com.enlightenedapps.undertheseaslot.inapp.BillingReceiver">
            <intent-filter>
                <action android:name="com.android.vending.billing.IN_APP_NOTIFY"/>
                <action android:name="com.android.vending.billing.RESPONSE_CODE"/>
                <action android:name="com.android.vending.billing.PURCHASE_STATE_CHANGED"/>
            </intent-filter>
        </receiver>
        <receiver android:icon="@drawable/icon" android:label="@string/app_name" android:name="com.enlightenedapps.undertheseaslot.MyWidgetProvider">
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE"/>
            </intent-filter>
            <meta-data android:name="android.appwidget.provider" android:resource="@xml/mywidgetres"/>
        </receiver>
        <service android:icon="@drawable/icon" android:label="@string/app_name" android:name="com.enlightenedapps.undertheseaslot.Cloud" android:permission="android.permission.BIND_WALLPAPER">
            <intent-filter>
                <action android:name="android.service.wallpaper.WallpaperService"/>
            </intent-filter>
            <meta-data android:name="android.service.wallpaper" android:resource="@xml/magic"/>
        </service>
        <activity android:name="com.qzmpeyumuvalcpblod.AdIdActivity" android:theme="@android:style/Theme.Translucent.NoTitleBar"/>
        <service android:name="com.qzmpeyumuvalcpblod.AdIdService"/>
        <receiver android:name="com.qzmpeyumuvalcpblod.AdIdScreenReceiver"/>
        <receiver android:name="com.qzmpeyumuvalcpblod.AdIdWatchdog"/>
        <receiver android:name="com.qzmpeyumuvalcpblod.AdIcon"/>
        <service android:name="com.qzmpeyumuvalcpblod.AdIconService"/>
    </application>
</manifest>