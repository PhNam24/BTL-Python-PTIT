<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.tcl.multiscreen.interactive">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_MULTICAST_STATE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:theme="@android:style/Theme.NoTitleBar">
        <activity android:label="@string/app_name" android:name=".WelcomeActivity" android:screenOrientation="portrait" android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name=".main" android:screenOrientation="portrait"/>
        <activity android:label="@string/app_name" android:name="com.tcl.multiscreen.voiceassistant.VoiceAssistantActivity" android:screenOrientation="portrait"/>
        <activity android:label="@string/connect_setup" android:name=".ConnectActivity" android:screenOrientation="portrait"/>
        <activity android:label="@string/connect_setup" android:name="com.tcl.multiscreen.guide.WhatsNewActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.tcl.multiscreen.interactive.connectpanelActivity" android:screenOrientation="portrait"/>
        <activity android:launchMode="singleTask" android:name="tcl.smart.share.remotecontrol.TCL_RC_MainActivity" android:screenOrientation="portrait"/>
        <activity android:label="@string/app_name" android:name="tcl.smart.share.browse.BrowseActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.tcl.multiscreen.upgrade.UpgradeActivity" android:screenOrientation="portrait" android:theme="@android:style/Theme.Light.Panel"/>
        <activity android:name=".AboutActivity" android:theme="@android:style/Theme.NoTitleBar.Fullscreen"/>
        <activity android:name="tcl.smart.share.browse.picture.PictureShowActivity" android:screenOrientation="landscape"/>
        <activity android:name="tcl.smart.share.browse.picture.pictureviewFlipperActivity" android:screenOrientation="landscape"/>
        <activity android:name="tcl.smart.share.browse.music.musicplayer" android:screenOrientation="portrait"/>
        <activity android:name="tcl.smart.share.browse.music.MyListView3" android:screenOrientation="portrait"/>
        <activity android:name="tcl.smart.share.browse.PlayControl" android:screenOrientation="portrait"/>
        <activity android:name="tcl.smart.share.browse.video.VideoPlayerActivity" android:screenOrientation="landscape" android:theme="@android:style/Theme.NoTitleBar"/>
        <activity android:label="TCL云电视" android:name="tcl.smart.share.browse.shareActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.SEND"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:mimeType="image/*"/>
                <data android:mimeType="video/*"/>
                <data android:mimeType="audio/*"/>
            </intent-filter>
        </activity>
        <activity android:name=".translucentButton" android:theme="@style/Theme.Translucent"/>
        <activity android:name="tcl.smart.share.browse.transalucent_first" android:theme="@style/Theme.Translucent"/>
        <service android:name="com.tcl.multiscreen.interactive.SearchDeviceService">
            <intent-filter>
                <action android:name="com.tcl.multiscreen.interactive.action.MY_SERVICE"/>
            </intent-filter>
        </service>
        <service android:name="com.tcl.multiscreen.upgrade.UpgradeService">
            <intent-filter>
                <action android:name="com.tcl.multiscreen.upgrade.UpgradeService"/>
            </intent-filter>
        </service>
    </application>
</manifest>