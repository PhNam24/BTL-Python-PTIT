<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="cn.com.pbzmbz.www">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:label="@string/app_name" android:name=".welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="瀑布之美壁纸特辑" android:name=".main" android:theme="@style/test"/>
        <activity android:label="瀑布之美" android:name=".imageAcitvity" android:theme="@style/test"/>
        <activity android:label="关于说明" android:name="cn.com.pbzmbz.www.aboutActivity" android:theme="@style/test"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="net.miidi.ad.banner.AdBannerActivity"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="net.miidi.ad.wall.AdWallActivity"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="net.crazymedia.iad.AdPushActivity"/>
        <receiver android:name="net.crazymedia.iad.AdPushReceiver">
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_ADDED"/>
                <data android:scheme="package"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>
        <service android:name="net.crazymedia.iad. AdPushService"/>
    </application>
    <uses-permission android:name="android.permission.SET_WALLPAPER"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
</manifest>